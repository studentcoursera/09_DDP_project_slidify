# Chunk 1: function
##=========================================##
## load_package/s:
## get the list of packages.
## parameter: packages
##
## For each package:
##         checks if package exists
##   if it doesnt, then, installs that package.
##   parameter: package_name
##=========================================##
load_packages <- function(pkgs) {
for (i in 1:length(pkgs)) load_package(pkgs[i])
}
load_package <- function(pkg) {
if (!isPackageInstalled(pkg)) install.packages(pkg,repos = "http://cran.us.r-project.org")
require(pkg, character.only = TRUE)
}
if (!is.element("R.utils", installed.packages())) install.packages("R.utils",repos = "http://cran.us.r-project.org")
require("R.utils")
load_packages(c("datasets","knitr","ggplot2","plyr"))
# Chunk 2: pkgs
## List of packages required
library(datasets)
library(knitr)
library(ggplot2)
library(plyr)
# Chunk 3: data
data(mtcars)
## neat way of doing data transformation - one statement
data3 <- cbind(mtcars[,c(1,3:7)],
sapply(mtcars[,c(2, 8, 10:11)], function(x) factor(x)),
transmission=factor(mtcars$am,labels=c('automatic','manual')))
summary.data <- function(x) {
temp <- c(min(x), mean(x) - sd(x), mean(x), mean(x) + sd(x), max(x))
names(temp) <- c("ymin", "lower", "middle", "upper", "ymax")		## geom_boxplot needs these values
temp
}
# Chunk 4: ploteda
p1 <- ggplot(data = data3, aes(x = transmission, y = mpg, fill = transmission)) +
stat_summary(fun.data = summary.data, geom = "boxplot") +
geom_jitter(position = position_jitter(width = 0.2), size=3, aes(colour = cyl)) +
ggtitle(expression(atop("Automatic vs Manual transmission", atop(italic("( in terms of mpg [miles per gallon] )")))))
